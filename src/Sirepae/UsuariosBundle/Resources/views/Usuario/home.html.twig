{% extends '::base.html.twig' %}

{% block body -%}
    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Navigación</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                {% set role = 'Usuario' %}
                {% if is_granted('ROLE_SUPER_ADMIN') %}
                    {% set icon = 'hospital-o' %}
                {% elseif is_granted('ROLE_COORDINADOR') %}
                    {% set icon = 'user-med' %}
                {% elseif is_granted('ROLE_DOCENTE') %}
                    {% set icon = 'medkit' %}
                {% elseif is_granted('ROLE_ESTUDIANTE') %}
                    {% set icon = 'stethoscope' %}
                {% endif %}
                            
                <a class="navbar-brand" href="{{ path('home') }}">
                    Sirepae
                    <i class="fa fa-{{ icon }}"></i>
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="{{ path('pae') }}">PAE</a>
                    </li>
                    <li>
                        <a href="{{ path('registros_enfermeria') }}">Registros de Enfermería</a>
                    </li>
                    <li>
                        <a href="{{ path('practica') }}">Prácticas</a>
                    </li>
                    <li class="dropdown">
                        <a data-toggle="dropdown" href="#" class="btn btn-primary">
                            Cuenta
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropDownMenu_cuenta">
                            <li>
                                <a href="{{ path('fos_user_profile_edit') }}">Perfil</a>
                            </li>
                            <li>
                                <a href="{{ path('fos_user_change_password') }}">Cambiar Contraseña</a>
                            </li>
                            <li role="presentation" class="divider"></li>
                            <li>
                                <a href="{{ path('fos_user_security_logout') }}">Salir</a>
                            </li>
                        </ul>
                    </li>
                </ul>
                <form class="navbar-form navbar-right">
                    <input type="text" class="form-control" placeholder="Search...">
                </form>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-3 col-md-2 sidebar">
                {% block menu %}
                <ul class="nav nav-sidebar">
                    <li class="{{ active_home_resumen is defined ?'active':''}}">
                        <a href="{{ path('home') }}">Vista General</a>
                    </li>
                    <li class="{{ active_practicas_resumen is defined ?'active':''}}">
                        <a href="{{ path('practica') }}">Administrar Practicas</a>
                    </li>
                    <li class="{{ active_usuarios_resumen is defined ?'active':''}}">
                        <a href="{{ path('usuario') }}">Administrar Usuarios</a>
                    </li>
                    <li class="{{ active_registrosEnfermeria_resumen is defined ?'active':''}}">
                        <a href="{{ path('registros_enfermeria') }}">Administrar Registros de Enfermería</a>
                    </li>
                    <li class="{{ active_paes_resumen is defined ?'active':''}}">
                        <a href="{{ path('pae') }}">Administrar Planes de Atención de Enfermería</a>
                    </li>
                </ul>
                {% endblock menu %}
                {% block submenu %}
                {% endblock submenu %}
            </div>
            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
                {% block content %}
                <h1 class="page-header">Tablero General - {{ "now"|date("Y") }}</h1>
                <div class="row placeholders">
                    <div class="col-xs-12 col-sm-4 placeholder">
                        {#<img class="img-responsive" alt="200x200" src="data:image/png;base64,">#}
                        <a href="{{ path('registros_enfermeria') }}">
                            <h4>Registros de Enfermería</h4>
                        </a>
                        <span class="text-muted"><span>{{ res | length }}</span> en total</span>
                    </div>
                    <div class="col-xs-12 col-sm-4 placeholder">
                        {#<img class="img-responsive" alt="200x200" src="data:image/png;base64,">#}
                        <a href="{{ path('pae') }}">
                            <h4>PAE</h4>
                        </a>
                        <span class="text-muted"><span>{{ res | length }}</span> en total</span>
                    </div>
                    <div class="col-xs-12 col-sm-4 placeholder">
                        {#<img class="img-responsive" alt="200x200" src="data:image/png;base64,">#}
                        <a href="{{ path('practica') }}">
                            <h4>Prácticas</h4>
                        </a>
                        <span class="text-muted"><span>{{ ps | length }}</span> en total</span>
                    </div>
                </div>
                
                <div class="col-sm-12 col-xs-12">
                    <h2 class="sub-header">Últimos Registros de Enfermería</h2> 
                    <div class="col-sm-12">
                        <div class="row">
                            {% for re in res|slice(0,8) %}
                                {% if loop.index is divisibleby(5) %}
                                    </div>
                                    <div class="row">
                                {% endif %}
                                <div class="col-sm-3">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            Registro de {{ re.estudiante }} al paciente {{ re.paciente }}
                                        </div>
                                        <div class="panel-body">
                                                {% for nota in re.notas|slice(0,6) %}
                                                <div class="panel panel-default">
                                                    <div class="panel-body">
                                                        {{ nota.nota }}
                                                    </div>
                                                </div>
                                                {% endfor %}
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-xs-12">
                    <h2 class="sub-header">Últimos PAE</h2> 
                    <div class="col-sm-12">
                        <div class="row">
                            {% for pae in paes|slice(0,6) %}
                                {% if loop.index is divisibleby(4) %}
                                    </div>
                                    <div class="row">
                                {% endif %}
                                <div class="col-sm-4">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            {{ pae }}
                                            {% if pae.calificacion is defined %}
                                                {% set valor = pae.calificacion.valor %}
                                                <span class="label label-{{ valor < 3?'danger':valor < 3.3?'warning':valor < 4?'info':'success' }}">{{ valor }}</span>
                                            {% endif %}
                                        </div>
                                        <div class="panel-body">
                                            <dl class="dl-horizontal">
                                                <dt>Valoración Objetica</dt>
                                                <dd>{{ pae.valObjetiva }}</dd>
                                            </dl>
                                            <dl class="dl-horizontal">
                                                <dt>Valoración Subjetiva</dt>
                                                <dd>{{ pae.valSubjetiva }}</dd>
                                            </dl>
                                            <dl class="dl-horizontal">
                                                <dt>Evaluación</dt>
                                                <dd>{{ pae.evaluacion }}</dd>
                                            </dl>
                                            {% if pae.calificacion is defined %}
                                                <dl class="dl-horizontal">
                                                    <dt>Docente</dt>
                                                    <dd>{{ pae.calificacion.docente }}</dd>
                                                </dl>
                                                <dl class="dl-horizontal">
                                                    <dt>Observacion Docente</dt>
                                                    <dd>{{ pae.calificacion.observacion }}</dd>
                                                </dl>
                                            {% endif %}
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-xs-12">
                    <h2 class="sub-header">Prácticas</h2> 
                    <div class="col-sm-12">
                        <div class="row">
                        {% for p in ps %}
                            {% if loop.index is divisibleby(4) %}
                                </div>
                                <div class="row">
                            {% endif %}
                            <div class="col-sm-4">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        Práctica {{ p.nombre }} 
                                        (<span class="text-muted">
                                            Docente {{ p.docente }}
                                        </span> y 
                                        <span class="text-muted">
                                            Coordinador {{ p.coordinador }}
                                        </span>)
                                    </div>
                                    <div class="panel-body">
                                        {{ p.nombre }}
                                    </div>
                                    {% if p.estudiantes is not empty %}
                                        <table>
                                            <thead>
                                                <tr>
                                                    <th>
                                                        Nombre Completo
                                                    </th>
                                                    <th>
                                                        Código
                                                    </th>
                                                    <th>
                                                        Total
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for e in p.estudiantes %}
                                                    <tr>

                                                    <td>
                                                        {{ e.usuario.fullNombre }}
                                                    </td>
                                                    <td>
                                                        {{ e.codigo }}
                                                    </td>
                                                    <td>
                                                        <div class="text-info title">
                                                            {{ e.registrosEnfermeria | length }} de Registros de Enfermería
                                                        </div>
                                                        <div class="text-info title">
                                                            {{ e.planesCuidado |length }} de Plantes de Cuidado
                                                        </div>
                                                    </td>
                                                </tr>
                                                {% endfor %}
                                            </tbody>
                                            <tfoot>
                                            </tfoot>

                                        </table>
                                    {% endif %}
                                </div>
                            </div>
                        {% endfor %}
                        </div>
                    </div>
                </div>
                {% endblock content %}
            </div>
        </div>
    </div>
{% endblock %}
